{
  "name": "@hessra/hessra-sdk",
  "version": "0.1.1",
  "description": "TypeScript SDK for Hessra Authorization Service",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hessra-Labs/hessra-sdk.ts.git"
  },
  "keywords": [
    "mTLS",
    "authorization",
    "authentication",
    "security",
    "typescript",
    "api-client"
  ],
  "author": "Hessra Labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Hessra-Labs/hessra-sdk.ts/issues"
  },
  "homepage": "https://github.com/Hessra-Labs/hessra-sdk.ts#readme",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "https-proxy-agent": "^7.0.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
